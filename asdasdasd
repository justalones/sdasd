---toilet fighting simulator---
local GUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/aaaa"))()

local UI = GUI:CreateWindow("TOILET FIGHTING SIMULATOR","BY Nameless Scripts")

local Home = UI:addPage("AUTO FARM",1,true,6)

Home:addLabel("CREATED BY NAMELESS SCRIPTS","")

local Home = UI:addPage("AUTO FARM",2,true,6)

Home:addLabel("AUTO FARM SECTION","Script was Created By Nameless Scritps")

local isToggleOn = false

Home:addToggle("INF CLICK", function(value)
    isToggleOn = value
    print("Toggle is now:", isToggleOn)
    
    if isToggleOn then
        -- Start the loop
        while isToggleOn do
            game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.EnemyService.RE.HitDummy:FireServer()
            wait()  -- Adjust the wait time as needed
        end
    end
end)

