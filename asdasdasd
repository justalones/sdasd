local isToggleOn = false

Home:addToggle("ARMED POLICE TOILET", function(value)
    isToggleOn = value
    print("Toggle is now:", isToggleOn)
    
    if isToggleOn then
        -- Start the loop
        while isToggleOn do
            local args = {
                [1] = "Area_1",
                [2] = "Batch_1",
                [3] = 1,
                [4] = workspace.Areas.Area_1.Enemies.Batch_1.Enemy_1
            }
            
            game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.EnemyService.RE.HitEnemy:FireServer(unpack(args))

            local args = {
                [1] = "Area_1",
                [2] = "Batch_1",
                [3] = 2,
                [4] = workspace.Areas.Area_1.Enemies.Batch_1.Enemy_1
            }
            
